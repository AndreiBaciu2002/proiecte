          <TableColumn fx:id="colLastName" prefWidth="100.0" text="Last Name" />
            <TableColumn fx:id="colDate" prefWidth="75.0" text="Date" />
        </columns>
      </TableView>
      <Button fx:id="removeButton" layoutX="265.0" layoutY="338.0" mnemonicParsing="false" onAction="#handle" text="Remove" />
